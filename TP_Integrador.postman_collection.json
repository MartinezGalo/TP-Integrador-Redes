				{
					"listen": "test",
					"script": {
						"exec": [
							"// Obtiene la respuesta en formato JSON\r",
							"var responseData = pm.response.json();\r",
							"\r",
							"// Extrae el serviceTicket de la respuesta\r",
							"var ticket = responseData.response.serviceTicket;\r",
							"\r",
							"// Guardar el ticket en una variable \"authToken\"\r",
							"pm.collectionVariables.set(\"authToken\", ticket);\r",
